{
  "bootstrap-sha": "",
  "last-release-sha": "",
  "plugins": ["node-workspace"],
  "packages": {
    ".": {
      "release-type": "simple",
      "component": "console-inline.nvim"
    },
    "packages/service": {
      "release-type": "node",
      "component": "@console-inline/service"
    }
  },
  "pull-request-title-pattern": "chore${scope}: release ${component} v${version}",
  "changelog-sections": [
    {
      "type": "feat",
      "section": "Features"
    },
    {
      "type": "fix",
      "section": "Bug Fixes"
    },
    {
      "type": "chore",
      "section": "Maintenance"
    },
    {
      "type": "docs",
      "section": "Documentation"
    }
  ],
  "labels": ["autorelease", "release-please"]
}
